{
  "name": "covid-vaccines-seniors",
  "version": "1.0.0",
  "description": "server for covid vaccines seniors web tool",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "start:watch": "nodemon",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benlambert43/COVID-Vaccines-Seniors.git"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "website-scraper": "^4.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@types/website-scraper": "^1.2.5",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.6.3",
    "babel-preset-env": "^1.7.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "superagent": "^6.1.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.1",
    "tslint": "^6.1.2",
    "typescript": "^4.1.5"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [
    "covid",
    "vaccination",
    "for",
    "senior",
    "citizens"
  ],
  "author": "ben lambert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benlambert43/COVID-Vaccines-Seniors/issues"
  },
  "homepage": "https://github.com/benlambert43/COVID-Vaccines-Seniors#readme"
}
