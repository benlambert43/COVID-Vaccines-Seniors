{
  "name": "covid-vaccines-seniors",
  "version": "1.0.0",
  "description": "server for covid vaccines seniors web tool",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "start:watch": "nodemon",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benlambert43/COVID-Vaccines-Seniors.git"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.25",
    "@types/supertest": "^2.0.10",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.1.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "superagent": "^5.3.1",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.3",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "keywords": [
    "covid",
    "vaccination",
    "for",
    "senior",
    "citizens"
  ],
  "author": "ben lambert",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/benlambert43/COVID-Vaccines-Seniors/issues"
  },
  "homepage": "https://github.com/benlambert43/COVID-Vaccines-Seniors#readme"
}
